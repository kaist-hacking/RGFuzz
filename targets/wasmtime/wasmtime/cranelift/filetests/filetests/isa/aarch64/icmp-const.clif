;; Test our lowerings that do things like `A >= B + 1 ==> A > B` to make better
;; use of immediate encodings.

test compile precise-output
set unwind_info=false
target aarch64

function %a(i32) -> i8 {
block0(v0: i32):
    v1 = iconst.i32 0x111001
    v2 = icmp.i32 uge v0, v1
    return v2
}

; VCode:
; block0:
;   subs wzr, w0, #1118208
;   cset x0, hi
;   ret
;
; Disassembled:
; block0: ; offset 0x0
;   cmp w0, #0x111, lsl #12
;   cset x0, hi
;   ret

