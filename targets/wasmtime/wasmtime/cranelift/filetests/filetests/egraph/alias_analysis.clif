test optimize
set opt_level=speed
target x86_64

function %f(i64) -> i64 {
block0(v0: i64):
    v1 = iconst.i64 0
    v2 = bor.i64 v0, v1
    v3 = load.i64 heap v0
    v4 = load.i64 heap v2
    v5 = band.i64 v3, v4
    store.i64 v0, v5
    v6 = load.i64 v3
    v7 = load.i64 v6
    return v7
}

; check: v3 = load.i64 heap v0
; check: store v0, v3
; check: v7 = load.i64 v0
; check: return v7
